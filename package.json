{
  "name": "electron-react-typescript-webpack-2021",
  "productName": "ERWT Boilerplate",
  "version": "5.0.0",
  "description": "Custom boilerplate for writing Desktop Applications using Electron, React, Webpack, TypeScript in 2020",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src/ --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "eslint --fix src/ --ext .ts,.js,.tsx,.jsx",
    "type-check": "tsc --noEmit",
    "migrate:songs": "dotenv -e .env -e workers/.env -- tsx scripts/migrate-songs-to-db.ts",
    "delete:songs": "dotenv -e .env -e workers/.env -- tsx scripts/delete-all-songs.ts"
  },
  "keywords": [
    "electron boilerplate",
    "minimal",
    "electron-webpack",
    "electron-react",
    "electron-typescript",
    "hmr",
    "boilerplate",
    "2020",
    "ERWT"
  ],
  "author": {
    "name": "codesbiome",
    "url": "https://github.com/codesbiome"
  },
  "contributors": [
    {
      "name": "guasam",
      "url": "https://github.com/guasam"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codesbiome/electron-react-webpack-typescript-2020"
  },
  "license": "MIT",
  "config": {
    "forge": "./tools/forge/forge.config.js"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251205.0",
    "@electron-forge/cli": "^7.0.0",
    "@electron-forge/maker-deb": "^7.0.0",
    "@electron-forge/maker-rpm": "^7.0.0",
    "@electron-forge/maker-squirrel": "^7.0.0",
    "@electron-forge/maker-zip": "^7.0.0",
    "@electron-forge/plugin-vite": "^7.0.0",
    "@types/node": "^16.11.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-modal": "^3.13.1",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.22",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^11.0.0",
    "electron": "^37.10.3",
    "eslint": "^8.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.21.0",
    "typescript": "^4.4.4",
    "vite": "^6.0.0",
    "wrangler": "^4.53.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "^2.87.0",
    "@tanstack/query-core": "4.36.1",
    "@tanstack/react-query": "4.36.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
