    "jotai": "^1.4.3",
    "lucide-react": "^0.556.0",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-css-transition-replace": "^4.0.5",
    "react-dom": "^19.0.0",
    "react-modal": "^3.14.4",
    "react-resizable-panels": "^3.0.6",
    "react-scroll": "^1.8.5",
    "react-textfit": "^1.1.1",
    "tailwind-merge": "^3.4.0",
    "textfit": "^2.4.0",
    "use-fit-text": "^2.4.0"
  }
}
