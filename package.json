{
  "name": "worship-view",
  "productName": "Worship View",
  "version": "1.4.1",
  "description": "Worship View is a desktop application for viewing and editing worship songs.",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src/ --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "eslint --fix src/ --ext .ts,.js,.tsx,.jsx",
    "type-check": "tsc --noEmit --project tsconfig.main.json && tsc --noEmit --project tsconfig.renderer.json && tsc --noEmit --project tsconfig.preload.json",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "build:test": "electron-forge package",
    "postinstall": "(cd node_modules/macos-alias 2>/dev/null && npx node-gyp rebuild) || true; (cd node_modules/fs-xattr 2>/dev/null && npx node-gyp rebuild) || true"
  },
  "keywords": [
    "worship",
    "songs",
    "view",
    "editor"
  ],
  "author": {
    "name": "AlexandruRoman",
    "url": "https://github.com/AlexandruRoman"
  },
  "contributors": [
    {
      "name": "AlexandruRoman",
      "url": "https://github.com/AlexandruRoman"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexandruRoman/worship-view.git"
  },
  "license": "MIT",
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "electron",
      "electron-winstaller",
      "esbuild"
    ]
  },
  "config": {
    "forge": "./tools/forge/forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.0.0",
    "@electron-forge/maker-deb": "^7.0.0",
    "@electron-forge/maker-dmg": "^7.0.0",
    "@electron-forge/maker-rpm": "^7.0.0",
    "@electron-forge/maker-squirrel": "^7.0.0",
    "@electron-forge/maker-zip": "^7.0.0",
    "@electron-forge/plugin-vite": "^7.0.0",
    "@electron-forge/publisher-github": "^7.0.0",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^16.11.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-modal": "^3.13.1",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.22",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^11.0.0",
    "electron": "^37.10.3",
    "eslint": "^8.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jazz-tools": "^0.19.18",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/query-core": "4.36.1",
    "@tanstack/react-query": "4.36.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "electron-squirrel-startup": "^1.0.1",
    "jotai": "^1.4.3",
    "lucide-react": "^0.556.0",
    "react": "^19.0.0",
    "react-css-transition-replace": "^4.0.5",
    "react-dom": "^19.0.0",
    "react-modal": "^3.14.4",
    "react-resizable-panels": "^3.0.6",
    "react-scroll": "^1.8.5",
    "react-textfit": "^1.1.1",
    "tailwind-merge": "^3.4.0",
    "textfit": "^2.4.0",
    "update-electron-app": "^3.1.2",
    "use-fit-text": "^2.4.0",
    "uuid": "^11.0.3"
  }
}