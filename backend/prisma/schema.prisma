generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model songs {
  id          Int    @id @default(autoincrement())
  title       String @db.VarChar(30)
  content     Json
  arrangement Json
  path        Json
  author      String @db.VarChar(30)
}
